  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "quiche-server",
      "cargo": {
        "args": [
          "build",
          "--bin",
          "quiche-server"
        ]
      },
      "env": {
        "RUST_LOG": "debug"
      },
      "args": [
        "--cert",
        "apps/src/bin/cert.crt",
        "--key",
        "apps/src/bin/cert.key",
        "--cc-algorithm",
        "bbr2",
        "--dgram-proto",
        "oneway",
        "--http-version",
        "HTTP/3",
      ]
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "quiche-client",
      "cargo": {
        "args": [
          "build",
          "--bin",
          "quiche-client"
        ]
      },
      "env": {
        "RUST_LOG": "debug"
      },
      "args": [
        "--connect-to",
        "127.0.0.1:4433",
        "--trust-origin-ca-pem",
        "quiche/examples/rootca.crt",
        "--dgram-proto",
        "oneway",
        "--http-version",
        "HTTP/3",
        "--method",
        "CONNECT",
        "--header",
        ":protocol: connect-udp",
        "https://quic.tech/masque/udp/127.0.0.1/2233",
      ]
    }
  ]
}